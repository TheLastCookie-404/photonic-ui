{
  "name": "photonic-ui",
  "version": "0.0.23",
  "description": "Ui components libruary for Vue3",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "main": "./dist/photonic-ui.umd.cjs",
  "module": "./dist/photonic-ui.js",
  "exports": {
    ".": {
      "import": "./dist/photonic-ui.js",
      "require": "./dist/photonic-ui.umd.cjs"
    }
  },
  "bin": {
    "photonic-ui": "dist/photonic-ui.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "execute": "npx vite-node ./src/index.ts",
    "publish-patch": "npm version patch && npm publish",
    "test": "vitest"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheLastCookie-404/photonic-ui.git"
  },
  "keywords": [
    "photonic",
    "photon"
  ],
  "author": "TheLastCookie-404",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TheLastCookie-404/photonic-ui/issues"
  },
  "homepage": "https://github.com/TheLastCookie-404/photonic-ui#readme",
  "dependencies": {
    "commander": "^14.0.0"
  }
}
